{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/impressum",
      "dest": "/impressum.html"
    },
    {
      "src": "/datenschutz", 
      "dest": "/datenschutz.html"
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
